{
 "display_name": "PySpark",
 "language": "python",
 "argv": [
  "/bin/python3",
  "-m",
  "ipykernel",
  "-f",
  "{connection_file}"
 ],
 "env": {
  "PYSPARK_PYTHON": "/bin/python3",
  "SPARK_HOME": "/usr/local/spark",
  "PYTHONPATH": "/usr/local/spark/python/lib/py4j-0.10.7-src.zip:/usr/local/spark/python/",
  "PYTHONSTARTUP": "/usr/local/spark/python/pyspark/shell.py",
  "PYSPARK_SUBMIT_ARGS": "--master spark:master_ip_address:7077 pyspark-shell"
 }
}
